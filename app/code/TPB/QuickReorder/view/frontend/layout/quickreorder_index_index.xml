    <body>
        <referenceContainer name="content">
            <block class="TPB\QuickReorder\Block\QuickReorder" name="reorder.product" cacheable="false" ifconfig="tpb_quickreorder/general/active" template="quickreorder.phtml" />
            <block class="Magento\Customer\Block\Account\Dashboard" name="customer.account.link.back" template="account/link/back.phtml"/>
        </referenceContainer>
    </body>
</page>